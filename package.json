{
  "name": "lergo-ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "grunt jshint && grunt test",
    "start": "grunt server",
    "prebuild": "npm run cleanup",
    "build": "jet steps --ci-build-id=001 build-lergo",
    "setup": "jet load lergo-build",
    "env:encrypt": "jet encrypt dev/env.decrypted env.encrypted",
    "env:decrypt": "jet decrypt env.encrypted dev/env.decrypted",
    "cleanup": "jet cleanup",
    "pretestme:decrypt": "npm run env:decrypt",
    "testme:decrypt": "source dev/env.decrypted && openssl aes-256-cbc -d -a -in scripts/testMe.json.enc -out dev/testMe.json -pass pass:$TESTME_KEY"
  },
  "devDependencies": {
    "connect-livereload": "~0.6.0",
    "grunt": "^1.3.0",
    "grunt-aws": "~0.6.2",
    "grunt-concurrent": "~2.3.1",
    "grunt-connect-proxy": "0.2.0",
    "grunt-contrib-clean": "~1.0.0",
    "grunt-contrib-coffee": "~1.0.0",
    "grunt-contrib-compass": "~1.1.1",
    "grunt-contrib-concat": "~1.0.1",
    "grunt-contrib-connect": "~1.0.2",
    "grunt-contrib-copy": "~1.0.0",
    "grunt-contrib-cssmin": "~2.0.0",
    "grunt-contrib-htmlmin": "~2.3.0",
    "grunt-contrib-jshint": "~0.6.5",
    "grunt-contrib-uglify": "~2.2.0",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-google-cdn": "~0.4.3",
    "grunt-html2js": "~0.3.8",
    "grunt-karma": "^3.0.2",
    "grunt-ng-annotate": "^3.0.0",
    "grunt-open": "~0.2.0",
    "grunt-rev": "~0.1.0",
    "grunt-sass": "~2.0.0",
    "grunt-usemin": "~0.1.11",
    "jshint-stylish": "~1.0.0",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coffee-preprocessor": "~1.0.1",
    "karma-coverage": "~1.1.1",
    "karma-failed-reporter": "0.0.3",
    "karma-firefox-launcher": "~1.0.0",
    "karma-html2js-preprocessor": "~1.1.0",
    "karma-jasmine": "~0.1.6",
    "karma-ng-html2js-preprocessor": "~1.0.0",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-requirejs": "~1.1.0",
    "karma-script-launcher": "~1.0.0",
    "log4js": "~1.1.0",
    "matchdep": "~1.0.1",
    "phantomjs-prebuilt": "^2.1.7",
    "requirejs": "^2.3.6"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
